{
  "name": "@ainotes/db",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "tsc --project tsconfig.build.json",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "db:studio": "prisma studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist coverage .turbo"
  },
  "dependencies": {
    "@prisma/client": "^6.8.0"
  },
  "devDependencies": {
    "@ainotes/config": "workspace:*",
    "@ainotes/core": "workspace:*",
    "@types/node": "^22.0.0",
    "eslint": "^9.18.0",
    "prisma": "^6.8.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
