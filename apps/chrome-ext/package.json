    "@crxjs/vite-plugin": "^2.0.0-beta.28",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/chrome": "^0.0.304",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.0",
    "vitest": "^2.1.8"
